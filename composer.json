{
    "name": "frosh/platform-share-basket",
    "version": "v0.1.0",
    "type": "shopware-platform-plugin",
    "description": "Shopware Plugin to share your basket",
    "license": "MIT",
    "authors": [
        {
            "name": "FriendsOfShopware",
            "homepage": "https://friendsofshopware.de"
        }
    ],
    "require": {
        "shopware/core": "*"
    },
    "extra": {
        "shopware-plugin-class": "Frosh\\ShareBasket\\FroshPlatformShareBasket",
        "copyright": "FriendsOfShopware",
        "label": {
            "de-DE": "Warenkorb teilen",
            "en-GB": "Share basket"
        },
        "description": {
            "de-DE": "Shopware Plugin, um Ihren Warenkorb zu teilen.",
            "en-GB": "Shopware Plugin to share your basket."
        }
    },
    "autoload": {
        "psr-4": {
            "Frosh\\ShareBasket\\": "src/"
        }
    },
    "suggest": {
        "shopware/storefront": "Require '*'.",
        "shopware/administration": "Require '*'."
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    }
}
